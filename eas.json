{
  "cli": { "appVersionSource": "remote" },
  "build": {
    "preview": {
      "android": { 
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease",
        "env": {
          "EAS_SKIP_AUTO_FINGERPRINT": "1"
        }
      }
    },
    "development": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "env": {
          "EAS_SKIP_AUTO_FINGERPRINT": "1"
        }
      }
    }
  }
}


